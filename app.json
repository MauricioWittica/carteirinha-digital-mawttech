{
  "expo": {
    "name": "app-prtb",
    "slug": "app-prtb",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "plugins":[
        [
          "expo-media-library",
          {
            "photosPermissions": "Allow $(MAWittech) to acess your photos.",
            "savePhotosPermissions": "Allow $(MAWittech) to save photos."
            
          }
        ]
      ]
    },
   
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
      
    },
    "android": {
      "package": "com.mawittech.app_prtb",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
       
      }
      
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": ""
  }
}
